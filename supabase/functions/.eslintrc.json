{
  "env": {
    "es2022": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off"
  },
  "globals": {
    "Deno": "readonly",
    "Response": "readonly",
    "Request": "readonly",
    "console": "readonly"
  }
}
